description "Gunicorn for Graphite"

start on runlevel [2345]
stop on runlevel [!2345]
kill timeout 5
respawn
export VENV=<%= node[:venv]%>
script
exec $VENV/bin/gunicorn_django --preload -w 2 --log-level debug --log-file $VENV/run/gunicorn.log -b 33.33.33.10 -p $VENV/run/gunicorn.pid /opt/graphite/webapp/graphite/settings.py
end script
